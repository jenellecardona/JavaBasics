package com.syntax.homework;

public class ArrayMethod {
    int printArraySum(int[] arr){
        int sum=0;
        for(int i=0; i<=3; i++) {
            sum+=arr[i];
        }
        return sum;
    }

    public static void main(String[] args) {
        ArrayMethod a=new ArrayMethod();
        System.out.println(a.printArraySum([4],6));
    }





}